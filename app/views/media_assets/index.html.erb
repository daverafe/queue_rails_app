<div class="container">
<h2>Search For Movies and Tv Shows</h2>
<hr>
<%= form_with model: @media_asset, method: :get do |f| %>
    <%= f.text_field :query %>
    <%= f.submit :Search, class: 'btn btn-light' %>
<% end %>


<p><%= link_to "Create A New Movie Or Tv Show", new_media_asset_path, :class => "link" %></p>



<ul>
<% @media_assets.each do |media_asset| %>
   <li><%= link_to media_asset.title, media_asset_path(media_asset), :class => "link" %> - <%= media_asset.media_type %></li>
<% end %>
</ul>
</div>