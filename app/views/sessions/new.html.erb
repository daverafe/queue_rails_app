<br>
<h3>Login!</h3> 

<%= form_with url: login_path do |f| %>
    <%=f.label :username %>
    <%=f.text_field :username %> <br>

    <%=f.label :password %>
    <%=f.password_field :password %> <br>

    <%=f.submit "Login" %> 
<% end %>


<h3>Sign In With Google</h3>

<%= button_to 'Google Sign In', '/auth/google_oauth2' %>