<div class="container">
<h2>Search For Users</h2>
<hr>
<%= form_with url: users_path, method: :get do |f| %>
    <%= f.text_field :query %>
    <%= f.submit "Search" %>
<% end %>

<ul>
<% @users.each do |user| %>
   <li><%= link_to user.username, user_path(user), :class => "link" %></li>
<% end %>
</ul>
</div>




